  "main": "app.js",
  "scripts": {
    "clean": "npm run rimraf -- dist compiled dll",
    
    "postinstallold": "npm run clean:bower && bower install && npm run build:aot",
  
    "start": "node app.js",
    "start:dev": "nodemon",
    "start:hmr": "npm run server:dev:hmr",
    "server:dev": "cross-env NODE_ENV=development node server.js",
    "server:dev:hmr": "npm run server:dev -- --inline --hot",
    "server:prod": "npm run build && cross-env NODE_ENV=production node server.js",
    "server:aot": "npm run build:aot && cross-env NODE_ENV=production node server.js",
    "build:dev": "npm run clean:dist && webpack --config config/webpack.dev.js --progress --profile",
    "build:aot": "npm run clean:dist && npm run clean:aot && cross-env BUILD_AOT=1 webpack --config config/webpack.prod.js  --progress --profile --bail",
    "watch": "npm run watch:dev",
    "watch:dev:hmr": "npm run watch:dev -- --hot",
    "watch:dev": "npm run build:dev -- --watch",
    "watch:prod": "npm run build:prod -- --watch",
    "clean:install": "npm set progress=false && npm install",
    "preclean:install": "npm run clean:all",
    "clean:all": "npm run rimraf -- node_modules dist compiled dll",
    "clean:bower": "npm run rimraf -- bower_components",
    "clean:dll": "npm run rimraf -- dll",
    "clean:aot": "npm run rimraf -- compiled",
    "clean:dist": "npm run rimraf -- dist",
    "rimraf": "rimraf",
    "lint": "npm run tslint \"public/**/*.ts\"",
    "tslint": "tslint",
    "webpack-dev-server": "webpack-dev-server",
    "webpack": "webpack"
  },
